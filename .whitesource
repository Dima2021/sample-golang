{
  "settingsInheritedFrom": "Dima2021/whitesource-config@master",
  "remediateSettings": {
    "workflowRules": {
      "enabled": false
    },
    "enableRenovate": true,
    "extends": [
      "config:base",
      "github>whitesource/merge-confidence:beta"
    ],
    "packageRules": [
      {
        "matchDatasources": ["golang-version"],
        "enabled": true
      },
      {
        "matchManagers": ["gomod"],
        "enabled": true,
        "matchDepTypes": ["indirect"],
        "registryUrls": ["https://proxy.golang.org"]
      }
    ],
    "customEnvVariables": {
      "GOPROXY": "https://proxy.golang.org,direct"
    },
   "hostRules": [
      {
        "hostType": "go",
        "matchHost": process.env.MVN_REGISTRY,
        "username": process.env.NEXUS_USERNAME,
        "password": process.env.NEXUS_PASSWORD
      }
    ]
  }
}
